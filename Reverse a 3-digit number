import java.io.*;
import java.util.*;

public class Solution {

    public static void main(String[] args) {
        Scanner co = new Scanner(System.in);
        int n = co.nextInt();
        int d,r = 0;
        while(n>0){
            d = n%10;
            r = r*10+d;
            n = n/10;
        }
        System.out.print(r);
}
}
